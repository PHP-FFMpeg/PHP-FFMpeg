{
    "name": "iakumai/php-ffmpeg",
    "type": "library",
    "description": "FFMpeg PHP, an Object Oriented library to communicate with AVconv / ffmpeg. Now supports Qt-faststart",
    "keywords": ["video processing", "video", "audio processing", "audio", "avconv", "ffmpeg", "avprobe", "ffprobe"],
    "license": "MIT",
    "authors": [
    {
        "name": "Valery Ozarnichuk",
        "email": "meru.no.akuma@gmail.com"
    },
    {
        "name": "Romain Neutron",
        "email": "imprec@gmail.com",
        "homepage": "http://www.lickmychip.com/"
    },
    {
        "name": "Phraseanet Team",
        "email": "info@alchemy.fr",
        "homepage": "http://www.phraseanet.com/"
    }
    ],
    "require": {
        "php"                      : ">=5.3.3",
        "alchemy/binary-driver"    : "~1.5",
        "doctrine/cache"           : "~1.0",
        "evenement/evenement"      : "~1.0"
    },
    "suggest": {
        "php-ffmpeg/extras"        : "A compilation of common audio & video drivers for PHP-FFMpeg"
    },
    "require-dev": {
        "sami/sami"       : "~1.0",
        "silex/silex"     : "~1.0",
        "phpunit/phpunit" : "~3.7"
    },
    "autoload": {
        "psr-0": {
            "FFMpeg": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    }
}
